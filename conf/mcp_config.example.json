{
    "mcpServers": {
      "filesystem": {
        "command": "npx",
        "args": [
          "-y",
          "@modelcontextprotocol/server-filesystem",
          "/path/to/Desktop",
          "/path/to/Downloads"
        ]
      },
      "echo-server": {
        "command": "/path/to/venv/bin/python",
        "args": [
          "/path/to/embedding-mcp-server/test/echo-mcp/simple_echo.py"
        ],
        "cwd": "/path/to/embedding-mcp-server"
      }
    }
  }